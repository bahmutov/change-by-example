{
  "name": "change-by-example",
  "description": "Finds a function that transforms a given object into another given object.",
  "version": "0.0.0-development",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/bahmutov/change-by-example/issues",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run deps",
        "npm test",
        "git add src/*.js",
        "npm run ban"
      ],
      "pre-push": [
        "npm run secure",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "src/*.js",
    "!src/*-spec.js"
  ],
  "homepage": "https://github.com/bahmutov/change-by-example#readme",
  "keywords": [
    "change",
    "json",
    "map",
    "mapper",
    "object",
    "shape",
    "shift"
  ],
  "license": "MIT",
  "main": "src/",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/change-by-example.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok && dependency-check --no-dev .",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "standard --verbose --fix src/*.js examples/*.js",
    "prelint": "npm run pretty",
    "pretest": "npm run lint",
    "pretty": "prettier-standard 'src/*.js' 'examples/*.js'",
    "secure": "nsp check",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run unit",
    "unit": "mocha src/*-spec.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "@bahmutov/all-paths": "1.0.2",
    "check-more-types": "^2.24.0",
    "debug": "2.6.9",
    "lazy-ass": "^1.6.0",
    "lodash": "4.17.20",
    "pluralize": "6.0.0",
    "ramda": "0.27.1",
    "variable-diff": "1.1.0"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.9.9",
    "dependency-check": "3.4.1",
    "deps-ok": "1.4.1",
    "dont-crack": "1.2.1",
    "git-issues": "1.3.1",
    "github-post-release": "1.13.1",
    "got": "8.3.2",
    "license-checker": "18.1.0",
    "mocha": "3.5.3",
    "next-update-travis": "1.7.1",
    "nsp": "2.8.1",
    "pre-git": "3.17.1",
    "prettier-standard": "6.0.0",
    "semantic-release": "6.3.6",
    "simple-commit-message": "3.3.2",
    "snap-shot": "2.17.0",
    "standard": "10.0.3"
  },
  "release": {
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release",
    "verifyRelease": {
      "path": "dont-crack",
      "test-against": []
    }
  }
}
